<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Marks_Within_Limit</fullName>
    <active>true</active>
    <errorConditionFormula>Marks_Obtained__c &gt; Maximum_Marks__c</errorConditionFormula>
    <errorDisplayField>Marks_Obtained__c</errorDisplayField>
    <errorMessage>Marks obtained cannot be greater than maximum marks.</errorMessage>
</ValidationRule>
